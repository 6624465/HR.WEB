@model  IEnumerable<HR.Web.Models.TravelClaimDetail>
 
                    <div class="row">
                        <div class="col-md-12">                            
                            <table class="table table-hover table-bordered">
                                <thead>
                                    <tr>
                                        <th>Date</th>
                                        <th>Perticulars</th>
                                        <th>Receipts (Y/N)</th>
                                        <th>Amount</th>
                                        <th>Currency</th>
                                        <th>Exchange Rate</th>
                                        <th>Total</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (var i = 0; i < Model.Count(); i++)
                                    {
                                        <tr>
                                            <td>@Model.ElementAt(i).TravelDate</td>
                                            <td>@Model.ElementAt(i).Perticulars</td>
                                            <td>@Model.ElementAt(i).Receipts</td>
                                            <td>@Model.ElementAt(i).Amount</td>
                                            <td>@Model.ElementAt(i).Currency</td>
                                            <td>@Model.ElementAt(i).ExchangeRate</td>
                                            <td>@Model.ElementAt(i).TotalInSGD</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>